# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = tab
indent_size = 4
# end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

# Ignore paths
[.gitpod.yml]
# charset = unset
# end_of_line = unset
# insert_final_newline = unset
# trim_trailing_whitespace = unset
indent_style = space
indent_size = 4